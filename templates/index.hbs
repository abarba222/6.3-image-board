
  <form class="insert-text">
    <input class="url-text" type="text" placeholder="Image URL">
    <textarea class="caption-text">Image Caption</textarea>
    <button class="cancel">Cancel</button>
    <input type="submit" value="ADD IMAGE">
  </form>

  <ul class="image-list">
    {{#each this}}
      <li><img src="{{url}}">
      <p>{{caption}}</p>
      </li>

    {{/each}}
  </ul>
